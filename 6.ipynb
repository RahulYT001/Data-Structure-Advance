   "source": [
    "count = 1\n",
    "str_return = \"\"\n",
    "a = input(\"Enter the string\\n\")\n",
    "length = len(a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b0189ff2",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(1, length):\n",
    "    if (a[i] == a[i-1]):\n",
    "        count += 1\n",
    "    else:\n",
    "        str_return += a[i-1] + f\"{count}\"\n",
    "        count = 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
