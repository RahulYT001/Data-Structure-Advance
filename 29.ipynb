   "source": [
    "a = input(\"Enter the elements with commas:\\n\")\n",
    "b = tuple(a.split(\",\"))\n",
    "ele = input(\"Enter one element: \\n\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f60183c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "c = 0\n",
    "for i in b:\n",
    "    if i == ele:\n",
    "        c += 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9551d596",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(f\"The number of occurrences of {ele} in {b} is {c}.\")"
   ]
  }
