   "source": [
    "a = int(input(\"Enter a number\\n\"))\n",
    "rev_num = 0\n",
    "num_copy = a\n",
    "while a > 0:\n",
    "    r = a % 10\n",
    "    rev_num = (rev_num * 10) + r\n",
    "    a = a // 10\n",
    "if num_copy == rev_num:\n",
    "    print(\"It's Palindrome\")\n",
    "else:\n",
    "    print(\"It's not a palindrome\")"
   ]
  }
