   "source": [
    "rev_str = \"\"\n",
    "length = len(a := input(\"Enter the string to be reversed: \"))\n",
    "while length > 0:\n",
    "    rev_str += a[length - 1]\n",
    "    length -= 1\n",
    "print(rev_str)"
   ]
  }
