    "list1 = list(j.strip() for j in a.split(\",\"))\n",
    "set1 = set(list1)\n",
    "d = {item: 0 for item in set1}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "57b7b04f",
   "metadata": {},
   "outputs": [],
   "source": [
    "for item in list1:\n",
    "    if item in d:\n",
    "        d[item] += 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
